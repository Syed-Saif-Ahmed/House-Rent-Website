{
    "version": 2,
    "engines": {
        "node": "18.x"
    },
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python",
            "config": {"runtime":"python3"}
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ]

}